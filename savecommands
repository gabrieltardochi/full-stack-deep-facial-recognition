python metaflow_train.py run --max_epochs=10 --learning_rate=1e-3 --early_stopping=TRUE --early_stopping_patience=3 --evals_per_epoch=1 --clip_grad_norm=TRUE --loss=batch-hard --train_batch_size=32 --augmentation_level=high --model_name=tf_efficientnet_b0.ns_jft_in1k
export METAFLOW_PROFILE=local AWS_ACCESS_KEY_ID=dummy_access_key AWS_SECRET_ACCESS_KEY=dummy_secret_key
export MF_RUN_ID=3
docker-compose -f docker-compose-api.yaml up --build -d
gunicorn -w 2 -b :8080 -t 300 -k uvicorn.workers.UvicornWorker --reload src.app.main:app
sudo chown 1000:1000 es-data
curl https://localhost:9200 -ku 'admin:admin'
curl -X DELETE "facial-recognition-es:9200/${ES_INDEX}"
curl -X DELETE "localhost:9200/people-faces-index"